<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>Calculadora</servlet-name>
        <servlet-class>PracticaJavaEE.Calculadora</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>TablaMultiplicar</servlet-name>
        <servlet-class>PracticaJavaEE.TablaMultiplicar</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>TablasMultiplicar</servlet-name>
        <servlet-class>PracticaJavaEE.TablasMultiplicar</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Login</servlet-name>
        <servlet-class>PracticaJavaEE.Login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LogOut</servlet-name>
        <servlet-class>PracticaJavaEE.LogOut</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Calculadora</servlet-name>
        <url-pattern>/Calc</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>TablaMultiplicar</servlet-name>
        <url-pattern>/Tabla</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>TablasMultiplicar</servlet-name>
        <url-pattern>/Tablas</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Login</servlet-name>
        <url-pattern>/Login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LogOut</servlet-name>
        <url-pattern>/LogOut</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
